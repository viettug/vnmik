export STAT_PREFIX='> '
export PKGSUFFIX='.r'

export VNMIKBIN_PATH=
export USERHOME=`cygpath -u "$USERPROFILE" 2>/dev/null`
export ISPOWERED=0

export GET_ME_RUN=0
